    <connection>Kaltura</connection>
    <sql>SELECT
  distinct nullif(upper(country),&apos;&apos;) country
, nullif(upper(state),&apos;&apos;) state
, nullif(upper(city), &apos;&apos;) city
, &apos;country&apos; country_name
, &apos;state&apos; state_name
, &apos;city&apos; city_name
FROM kuser
where country is not null
	and updated_at &gt; now() - interval ${UpdateDaysInterval} day
</sql>
