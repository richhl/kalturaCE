    <entry>
      <name>Reset storage_usage aggregation</name>
      <description>SQL</description>
      <type>SQL</type>
      <sql>UPDATE kalturadw.aggr_managment a, 
		(SELECT DISTINCT DATE(updated_at) date_id FROM kalturadw.dwh_dim_file_sync, (SELECT (MAX(end_time)) last_execution FROM kalturadw.aggr_managment
			WHERE aggr_name = &apos;storage_usage&apos;
			AND is_calculated = 1) aggr_m
		WHERE dwh_dim_file_sync.dwh_creation_date &gt; last_execution 
		AND original = 1) dates
	SET a.is_calculated=0 WHERE a.aggr_day_int = dates.date_id
	AND a.aggr_name = &apos;storage_usage&apos;;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <connection>KalturaDW</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
