        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here
var event_time = new Date();
var error_reason_code=0;
var error_reason=null;

&#47;&#47;try
{
        event_time = str2date ( event_time_str , &quot;yyyy-MM-dd HH:mm:ss&quot; );

        &#47;&#47; validate entry_type_id is not null
        if ( event_type_id == null )
        {
                error_reason_code=1;
                error_reason=&quot;missing event type&quot;;
        }
        else if ( year(event_time)&gt;2020 ) &#47;&#47; validate event time is less than 2020
        {
                error_reason_code=2;
                error_reason=&quot;date after 2020&#47;12&#47;31 00:00:00&quot;;
        }
        else if ( year(event_time)&lt;2009 )  &#47;&#47; validate event time is more than 2009
        {
                error_reason_code=3;
                error_reason=&quot;date before 2009&#47;01&#47;01  00:00:00&quot;;
        }
        else if ( substr(entry_id,0,5)==&quot;http:&quot; || substr(entry_id,0,6)==&quot;https:&quot; )  &#47;&#47; validate the entry id does not start with http
        {
                error_reason_code=4;
                error_reason=&quot;entry id starts with http&quot;;
        }
        else if ( substr(entry_id,0,5)==&quot;rtmp:&quot;  )  &#47;&#47; validate the entry id does not start with rtmp
        {
                error_reason_code=6;
                error_reason=&quot;entry id starts with rtmp&quot;;
        }
		else if ( session_id==&quot;false&quot; )
		{
				error_reason_code=7;
                error_reason=&quot;invalid session id&quot;;
		}
}
&#47;*
catch ( ex )
{
        error_reason_code=5;
        error_reason=&quot;event time format [&quot; + ex.toString() + &quot;]&quot;;
}
*&#47;


</jsScript_script>
