&#47;&#47; Parse a queryString into a dictionary
function parseQueryString(s){
	&#47;&#47;sVars = s.split(&apos;&amp;&apos;);
	&#47;&#47;var sDictionaryDefinition = &quot;&quot;;
	&#47;&#47;firstRow = true;
	&#47;&#47;for (i=0;i&lt;sVars.length;i++) {
	&#47;&#47;	keyValuePair = sVars[i];
	&#47;&#47;	seperatorLoaction = indexOf(keyValuePair, &quot;=&quot;);
	&#47;&#47;	if (seperatorLoaction &gt; - 1) {
	&#47;&#47;		key = substr(keyValuePair, 0, seperatorLoaction);
	&#47;&#47;		value = substr(keyValuePair, seperatorLoaction + 1);
	&#47;&#47;		if (!firstRow) {
	&#47;&#47;			sDictionaryDefinition += &quot;,&quot;;
	&#47;&#47;		} else {
	&#47;&#47;			firstRow = false;
	&#47;&#47;		}
	&#47;&#47;		&#47;&#47; Handle single quotes
	&#47;&#47;		value = replace(value, &apos;\&apos;&apos;,&apos;\\\\\\\&apos;&apos;);
	&#47;&#47;		sDictionaryDefinition += &quot;&apos;&quot; + key + &quot;&apos;&quot; + &quot;:&quot; +  &quot;&apos;&quot; +  value +  &quot;&apos;&quot;;
	&#47;&#47;	}
	&#47;&#47;}
	&#47;&#47;eval(&quot;sDictionary = {&quot; + sDictionaryDefinition + &quot;};&quot;);
	var sDictionary = {};
	s.replace(&#47;([^=&amp;]+)=([^&amp;]*)&#47;g, function(m, key, value){ 
        sDictionary[key] = value; 
    }); 
	return sDictionary;
}

function getValue(dictionary, key){
	var val = dictionary[keyPrefix + key];
	return val ? replace(val, &apos;,&apos;, &apos;~&apos;) : &quot;&quot;;
	&#47;&#47;return val ? val : &quot;&quot;;
}
